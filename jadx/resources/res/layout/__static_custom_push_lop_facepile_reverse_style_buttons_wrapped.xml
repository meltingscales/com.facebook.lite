<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@color/__static_notification_custom_layout_background" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
        <LinearLayout android:orientation="vertical" android:id="@+id/custom_push_lop_facepile_reverse_style_buttons_wrapped" android:paddingLeft="@dimen/cl_left" android:paddingRight="@dimen/cl_right" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/cl_top">
            <LinearLayout android:orientation="horizontal" android:id="@+id/header" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/cl_header_left" android:layout_marginBottom="@dimen/cl_header_bottom" android:layout_marginStart="@dimen/cl_header_left">
                <ImageView android:layout_gravity="center_vertical" android:id="@+id/small_icon" android:layout_width="@dimen/cl_icon_size" android:layout_height="@dimen/cl_icon_size" android:layout_marginRight="@dimen/cl_icon_right" android:tint="@color/__static_notification_custom_layout_blue_color" android:layout_marginEnd="@dimen/cl_icon_right"/>
                <TextView android:textSize="12sp" android:textColor="@color/__static_notification_custom_layout_blue_color" android:layout_gravity="center_vertical" android:id="@+id/app_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/cl_app_name_right" android:layout_marginBottom="@dimen/cl_header_text_bottom" android:layout_marginEnd="@dimen/cl_app_name_right"/>
                <LinearLayout android:orientation="horizontal" android:id="@+id/subtext_separator" android:layout_width="0dp" android:layout_height="18sp" android:layout_weight="2">
                    <ImageView android:layout_gravity="center" android:background="@drawable/custom_push_notifications_dot" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/cl_header_right_space" android:layout_marginEnd="@dimen/cl_header_right_space"/>
                    <TextView android:textSize="12sp" android:textColor="@color/__static_notification_custom_layout_info_color" android:ellipsize="end" android:id="@+id/subtext" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/cl_header_right_space" android:maxLines="1" android:layout_marginEnd="@dimen/cl_header_right_space"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/timestamp_separator" android:layout_width="wrap_content" android:layout_height="18sp" android:layout_marginRight="@dimen/cl_header_right_space" android:layout_marginEnd="@dimen/cl_header_right_space">
                    <ImageView android:layout_gravity="center" android:background="@drawable/custom_push_notifications_dot" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                </FrameLayout>
                <TextView android:textSize="12sp" android:textColor="@color/__static_notification_custom_layout_info_color" android:layout_gravity="center_vertical" android:id="@+id/timestamp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/cl_header_text_bottom"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <FrameLayout android:layout_gravity="center_horizontal" android:id="@+id/actor_image_holder" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <FrameLayout android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <ImageView android:layout_gravity="center_horizontal" android:id="@+id/actor_image" android:layout_width="@dimen/cl_image_size" android:layout_height="@dimen/cl_image_size" android:layout_marginLeft="@dimen/cl_image_ring_margin" android:layout_marginTop="@dimen/cl_image_ring_margin" android:layout_marginRight="@dimen/cl_image_ring_margin" android:layout_marginBottom="@dimen/cl_image_ring_margin" android:shape="oval" android:layout_marginStart="@dimen/cl_image_ring_margin" android:layout_marginEnd="@dimen/cl_image_ring_margin"/>
                        <ImageView android:layout_gravity="center_horizontal" android:background="@drawable/custom_push_notifications_actor_border" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/cl_image_ring" android:layout_marginTop="@dimen/cl_image_ring" android:layout_marginRight="@dimen/cl_image_ring" android:layout_marginBottom="@dimen/cl_image_ring" android:layout_marginStart="@dimen/cl_image_ring" android:layout_marginEnd="@dimen/cl_image_ring"/>
                        <FrameLayout android:layout_gravity="end|bottom" android:id="@+id/image_holder" android:layout_width="wrap_content" android:layout_height="wrap_content">
                            <ImageView android:layout_gravity="center_horizontal" android:id="@+id/image" android:layout_width="@dimen/cl_small_image_size" android:layout_height="@dimen/cl_small_image_size" android:layout_marginLeft="@dimen/cl_image_ring_margin" android:layout_marginTop="@dimen/cl_image_ring_margin" android:layout_marginRight="@dimen/cl_image_ring_margin" android:layout_marginBottom="@dimen/cl_image_ring_margin" android:shape="oval" android:layout_marginStart="@dimen/cl_image_ring_margin" android:layout_marginEnd="@dimen/cl_image_ring_margin"/>
                            <ImageView android:layout_gravity="center_horizontal" android:background="@drawable/custom_push_notifications_small_actor_border" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/cl_image_ring" android:layout_marginTop="@dimen/cl_image_ring" android:layout_marginRight="@dimen/cl_image_ring" android:layout_marginBottom="@dimen/cl_image_ring" android:layout_marginStart="@dimen/cl_image_ring" android:layout_marginEnd="@dimen/cl_image_ring"/>
                            <ImageView android:layout_gravity="center" android:background="@drawable/custom_push_notifications_small_inner_actor_border" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                        </FrameLayout>
                    </FrameLayout>
                </FrameLayout>
                <FrameLayout android:layout_gravity="center_horizontal" android:id="@+id/facepiple_reverse_recip_image_only_holder" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <FrameLayout android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <ImageView android:layout_gravity="center_horizontal" android:id="@+id/facepiple_reverse_recip_image_only" android:layout_width="@dimen/cl_image_size" android:layout_height="@dimen/cl_image_size" android:layout_marginLeft="@dimen/cl_image_ring_margin" android:layout_marginTop="@dimen/cl_image_ring_margin" android:layout_marginRight="@dimen/cl_image_ring_margin" android:layout_marginBottom="@dimen/cl_image_ring_margin" android:shape="oval" android:layout_marginStart="@dimen/cl_image_ring_margin" android:layout_marginEnd="@dimen/cl_image_ring_margin"/>
                        <ImageView android:layout_gravity="center_horizontal" android:background="@drawable/custom_push_notifications_actor_border" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/cl_image_ring" android:layout_marginTop="@dimen/cl_image_ring" android:layout_marginRight="@dimen/cl_image_ring" android:layout_marginBottom="@dimen/cl_image_ring" android:layout_marginStart="@dimen/cl_image_ring" android:layout_marginEnd="@dimen/cl_image_ring"/>
                    </FrameLayout>
                </FrameLayout>
                <LinearLayout android:orientation="vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/cl_text_left" android:layout_marginStart="@dimen/cl_text_left">
                    <TextView android:textSize="14sp" android:textColor="@color/__static_notification_custom_layout_realtitle_color" android:ellipsize="end" android:id="@+id/realtitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1"/>
                    <TextView android:textSize="14sp" android:textColor="@color/__static_notification_custom_layout_realtitle_color" android:ellipsize="end" android:id="@+id/subtitle" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1"/>
                    <TextView android:textSize="14sp" android:textColor="@color/__static_notification_custom_layout_title_color" android:ellipsize="end" android:layout_gravity="center_vertical" android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxLines="1"/>
                    <TextView android:textSize="14sp" android:textColor="@color/__static_notification_custom_layout_title_color" android:layout_gravity="center_vertical" android:id="@+id/titleMultiline" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:id="@+id/filler" android:layout_width="match_parent" android:layout_height="wrap_content">
                <ImageView android:layout_width="match_parent" android:layout_height="@dimen/cl_button_filler"/>
            </FrameLayout>
        </LinearLayout>
    </RelativeLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/buttonfiller" android:layout_width="match_parent" android:layout_height="wrap_content">
        <ImageView android:layout_width="match_parent" android:layout_height="@dimen/cl_button_filler"/>
    </LinearLayout>
</LinearLayout>
